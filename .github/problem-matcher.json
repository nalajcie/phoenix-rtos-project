{
    "problemMatcher": [
        {
            "owner": "custom-problem-matcher",
            "pattern": [
                {
                    "regexp": "^--- Building (.*) ---$",
                    "fromPath": 1
                },
                {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(?:fatal\\s+)?(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            ]
        }
    ]
}
